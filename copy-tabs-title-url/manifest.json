{
  "name": "Copy Tabs Title URL",
  "version": "0.1.0",
  "description": "Copy the title and URL from a Chrome tabs",
  "icons": {
    "16": "icon/icon16x16.png",
    "128": "icon/icon128x128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon/icon16x16.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "manifest_version": 3
}
